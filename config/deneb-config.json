{
    "$schema": "../schema/deneb-config.json",
    "features": {
        "developerMode": false,
        "enableExternalUri": false,
        "fetchMoreData": true,
        "selectionContextMenu": true,
        "selectionDataPoint": true,
        "selectionCrossHighlight": false,
        "tooltipHandler": true,
        "tooltipResolveNumberFieldFormat": true,
        "useDownloadApi": false,
        "templateExportPreviewImages": false,
        "fx": "2022-03-31",
        "unitTestCanary": false
    },
    "fluentUiTheme": {
        "palette": {
            "themePrimary": "#f2c811",
            "themeLighterAlt": "#fefdf5",
            "themeLighter": "#fdf6d7",
            "themeLight": "#fbeeb5",
            "themeTertiary": "#f7de6d",
            "themeSecondary": "#f4cf2c",
            "themeDarkAlt": "#dab50f",
            "themeDark": "#b8990d",
            "themeDarker": "#887109",
            "neutralLighterAlt": "#f3f2f1",
            "neutralLighter": "#eaeaea",
            "neutralLight": "#dadada",
            "neutralQuaternaryAlt": "#dadada",
            "neutralQuaternary": "#d0d0d0",
            "neutralTertiaryAlt": "#c8c8c8",
            "neutralTertiary": "#c2c2c2",
            "neutralSecondary": "#666666",
            "neutralPrimaryAlt": "#4b4b4b",
            "neutralPrimary": "#252423",
            "neutralDark": "#252423",
            "black": "#252423",
            "white": "#ffffff",
            "redDark": "#a4262c"
        }
    },
    "keyBindings": {
        "applyChanges": {
            "combination": "ctrl|enter",
            "functionalArea": "other"
        },
        "autoApplyToggle": {
            "combination": "ctrl|shift|enter",
            "functionalArea": "other"
        },
        "repairFormatJson": {
            "combination": "ctrl|alt|r",
            "functionalArea": "other"
        },
        "newTemplate": {
            "combination": "ctrl|alt|t",
            "functionalArea": "other"
        },
        "newSpecification": {
            "combination": "ctrl|alt|n",
            "functionalArea": "other"
        },
        "mapFields": {
            "combination": "ctrl|alt|f",
            "functionalArea": "other"
        },
        "openHelpUrl": {
            "combination": "ctrl|alt|h",
            "functionalArea": "other"
        },
        "navigateSpecification": {
            "combination": "ctrl|alt|1",
            "functionalArea": "other"
        },
        "navigateConfig": {
            "combination": "ctrl|alt|2",
            "functionalArea": "other"
        },
        "navigateSettings": {
            "combination": "ctrl|alt|3",
            "functionalArea": "other"
        },
        "toggleEditorPane": {
            "combination": "ctrl|alt|space",
            "functionalArea": "other"
        },
        "editorFocusOut": {
            "combination": "esc",
            "functionalArea": "other"
        },
        "zoomIn": {
            "combination": "ctrl|alt|num_add,ctrl|alt|=",
            "functionalArea": "other"
        },
        "zoomOut": {
            "combination": "ctrl|alt|num_subtract,ctrl|alt|-",
            "functionalArea": "other"
        },
        "zoomReset": {
            "combination": "ctrl|alt|0,ctrl|alt|num_0",
            "functionalArea": "other"
        },
        "zoomFit": {
            "combination": "ctrl|alt|num_multiply",
            "functionalArea": "other"
        }
    },
    "modalDialog": {
        "heightPercent": 0.95,
        "widthPercent": 0.95
    },
    "providerResources": {
        "deneb": {
            "interactivityDocumentationUrl": "https://deneb-viz.github.io/interactivity-overview",
            "changelogDocumentationUrl": "https://deneb-viz.github.io/changelog",
            "legacyVersion": "1.0.0.57"
        },
        "vega": {
            "documentationUrl": "https://vega.github.io/vega/docs/",
            "schemaUrl": "https://vega.github.io/schema/vega/v5.json",
            "legacyVersion": "5.21.0",
            "patch": {
                "signals": [
                    {
                        "name": "pbiContainerHeight",
                        "update": "containerSize()[1]"
                    },
                    {
                        "name": "pbiContainerWidth",
                        "update": "containerSize()[0]"
                    }
                ]
            }
        },
        "vegaLite": {
            "documentationUrl": "https://vega.github.io/vega-lite/docs/",
            "schemaUrl": "https://vega.github.io/schema/vega-lite/v5.json",
            "legacyVersion": "5.1.1"
        }
    },
    "propertyDefaults": {
        "dataLimit": {
            "override": false,
            "showCustomVisualNotes": true
        },
        "developer": {
            "locale": "en-US",
            "showVersionNotification": false
        },
        "editor": {
            "debounceInterval": 200,
            "tabSize": 2,
            "position": "left",
            "fontSize": 10,
            "wordWrap": true,
            "showGutter": true,
            "showLineNumbers": true,
            "showViewportMarker": true,
            "maxLineLength": 40
        },
        "vega": {
            "jsonSpec": null,
            "jsonConfig": "{}",
            "provider": "vegaLite",
            "renderMode": "svg",
            "enableContextMenu": true,
            "enableTooltips": true,
            "enableSelection": false,
            "selectionMaxDataPoints": 50,
            "tooltipDelay": 0,
            "isNewDialogOpen": true
        },
        "theme": {
            "ordinalColorCount": 10,
            "ordinalColorCountMin": 1,
            "ordinalColorCountMax": 100
        },
        "performance": {
            "enableResizeRecalc": true
        }
    },
    "selection": {
        "minDataPointsValue": 1,
        "maxDataPointsValue": 250,
        "dataPointsStepValue": 1
    },
    "splitPaneDefaults": {
        "minSize": 350,
        "defaultSizePercent": 0.4,
        "maxSizePercent": 0.6,
        "collapsedSize": 36
    },
    "svgFilters": {
        "none": {
            "displayName": "None",
            "attributes": [],
            "effects": []
        },
        "xkcd": {
            "displayName": "Deneb.XKCD",
            "ignoreText": true,
            "attributes": [
                {
                    "name": "filterUnits",
                    "value": "userSpaceOnUse"
                },
                {
                    "name": "x",
                    "value": "-5"
                },
                {
                    "name": "y",
                    "value": "-5"
                },
                {
                    "name": "width",
                    "value": "100%"
                },
                {
                    "name": "height",
                    "value": "100%"
                }
            ],
            "effects": [
                {
                    "primitive": "feTurbulence",
                    "attributes": [
                        {
                            "name": "type",
                            "value": "fractalNoise"
                        },
                        {
                            "name": "baseFrequency",
                            "value": "0.05"
                        },
                        {
                            "name": "result",
                            "value": "noise"
                        }
                    ]
                },
                {
                    "primitive": "feDisplacementMap",
                    "attributes": [
                        {
                            "name": "scale",
                            "value": "5"
                        },
                        {
                            "name": "xChannelSelector",
                            "value": "R"
                        },
                        {
                            "name": "yChannelSelector",
                            "value": "G"
                        },
                        {
                            "name": "in",
                            "value": "SourceGraphic"
                        },
                        {
                            "name": "in2",
                            "value": "noise"
                        }
                    ]
                }
            ]
        }
    },
    "templates": {
        "metadataVersion": 1,
        "previewImageSize": 150
    },
    "visualViewPortAdjust": {
        "top": 5,
        "left": 5
    },
    "zoomLevel": {
        "min": 10,
        "max": 300,
        "step": 10,
        "default": 100
    }
}
