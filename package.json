{
    "name": "@deneb-viz/deneb",
    "author": "Deneb contributors",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/deneb-viz/deneb.git"
    },
    "packageManager": "npm@10.3.0",
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build": "turbo build",
        "clean": "turbo run clean",
        "dev": "turbo run dev webpack:start --parallel --concurrency=25",
        "package": "npm run validate-config-for-commit && turbo build webpack:package",
        "eslint": "turbo run eslint -- && npm run -s eslint:root",
        "test": "turbo run test",
        "test:watch": "turbo run test:watch",
        "package-standalone": "cross-env DOTENVX_ENV=.env.standalone npm run build && cross-env DOTENVX_ENV=.env.standalone npm run package:standalone",
        "package-alpha": "npm run build && npm run package:alpha",
        "package-beta": "npm run build && npm run package:beta",
        "install-cert": "pbiviz install-cert",
        "eslint:root": "npx eslint .",
        "webpack:start": "cross-env NODE_ENV=development webpack serve --config webpack.dev.config.js",
        "webpack:build": "cross-env NODE_ENV=development webpack --config webpack.dev.config.js",
        "webpack:package": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
        "webpack:analyze": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js --profile --json > webpack-stats.json",
        "package:standalone": "ts-node -P tsconfig.build-scripts.json bin/package-custom --mode standalone",
        "package:alpha": "ts-node -P tsconfig.build-scripts.json bin/package-custom --mode alpha",
        "package:beta": "ts-node -P tsconfig.build-scripts.json bin/package-custom --mode beta",
        "prettier-check": "prettier --config .prettierrc {src,spec,style}/**/{*.ts*,*.css,*.less} --check",
        "prettier-format": "prettier --config .prettierrc {src,spec,style}/**/{*.ts*,*.css,*.less} --write",
        "sync-packages": "syncpack fix-mismatches && npm i",
        "validate-config-for-commit": "ts-node -P tsconfig.build-scripts.json bin/validate-config-for-commit",
        "validate-packages-sync": "syncpack list-mismatches",
        "sync:metadata": "ts-node -P tsconfig.build-scripts.json bin/sync-package-metadata.ts"
    },
    "devDependencies": {
        "@babel/core": "^7.28.5",
        "@babel/preset-env": "^7.28.5",
        "@babel/preset-react": "^7.28.5",
        "@babel/runtime": "^7.28.4",
        "@babel/runtime-corejs3": "^7.28.4",
        "@deneb-viz/app-core": "*",
        "@deneb-viz/configuration": "*",
        "@deneb-viz/data-core": "*",
        "@deneb-viz/json-processing": "*",
        "@deneb-viz/powerbi-compat": "*",
        "@deneb-viz/template-usermeta": "*",
        "@deneb-viz/utils": "*",
        "@deneb-viz/vega-runtime": "*",
        "@dotenvx/dotenvx": "^1.51.1",
        "@fluentui/react-components": "^9.72.6",
        "@fluentui/react-icons": "^2.0.313",
        "@types/d3": "^7.1.0",
        "@types/jsum": "^0.1.0",
        "@types/lodash": "^4.14.202",
        "@types/minimist": "^1.2.5",
        "@types/node": "^22.15.33",
        "@types/react": "^19.0.0",
        "@types/react-dom": "^19.0.0",
        "@types/simple-sha1": "^3.0.3",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "adm-zip": "^0.5.5",
        "babel-loader": "^10.0.0",
        "buffer": "^6.0.3",
        "cross-env": "^10.1.0",
        "css-loader": "^7.1.2",
        "eslint": "^8.46.0",
        "eslint-plugin-powerbi-visuals": "^1.0.0",
        "extra-watch-webpack-plugin": "^1.0.3",
        "fast-equals": "^5.3.3",
        "git-last-commit": "^1.0.1",
        "json-loader": "^0.5.7",
        "json-stringify-pretty-compact": "^4.0.0",
        "less": "^4.4.2",
        "less-loader": "^12.3.0",
        "lodash": "^4.17.21",
        "mergician": "^2.0.2",
        "mini-css-extract-plugin": "^2.9.4",
        "minimist": "^1.2.5",
        "powerbi-visuals-api": "5.11.0",
        "powerbi-visuals-tools": "^7.0.1",
        "powerbi-visuals-utils-formattingutils": "^6.1.2",
        "powerbi-visuals-webpack-plugin": "^4.3.1",
        "prettier": "^3.3.3",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "syncpack": "^12.3.2",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "tsup": "^8.5.0",
        "turbo": "^2.6.0",
        "typescript": "^5.6.2",
        "vega": "6.2.0",
        "vega-lite": "6.4.1",
        "webpack": "^5.102.1",
        "webpack-bundle-analyzer": "^4.10.2",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2",
        "webpack-merge": "^6.0.1",
        "zustand": "^4.5.5"
    },
    "peerDependencies": {
        "monaco-editor": "0.45.0"
    }
}