{
    "name": "@deneb-viz/core-dependencies",
    "version": "1.7.0",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/deneb-viz/sadr.git"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./constants": {
            "types": "./dist/constants/index.d.ts",
            "default": "./dist/constants/index.js"
        },
        "./dataset": {
            "types": "./dist/dataset/index.d.ts",
            "default": "./dist/dataset/index.js"
        },
        "./definitions": {
            "types": "./dist/definitions/index.d.ts",
            "default": "./dist/definitions/index.js"
        },
        "./json": {
            "types": "./dist/json/index.d.ts",
            "default": "./dist/json/index.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "default": "./dist/utils/index.js"
        }
    },
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "tsup": {
        "entry": [
            "src/**/*.ts",
            "!src/**/*.test.ts"
        ],
        "dts": true,
        "format": [
            "cjs",
            "esm"
        ],
        "onSuccess": "npx tsc --emitDeclarationOnly --declaration"
    },
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "test": "jest",
        "test:watch": "jest --watch",
        "eslint": "eslint"
    },
    "jest": {
        "preset": "@deneb-viz/jest-presets/jest/node"
    },
    "dependencies": {
        "vega": "6.2.0",
        "vega-lite": "6.4.1"
    },
    "devDependencies": {
        "@deneb-viz/eslint-config": "*",
        "@deneb-viz/jest-presets": "*",
        "@deneb-viz/typescript-config": "*",
        "powerbi-visuals-api": "5.11.0"
    }
}