{
    "name": "@deneb-viz/integration-powerbi",
    "version": "1.7.0",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/deneb-viz/sadr.git"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "type": "module",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "browser": {
        "canvas": false,
        "encoding": false,
        "fs": false,
        "fs/promises": false,
        "net": false,
        "tls": false
    },
    "tsup": {
        "entry": [
            "src/**/*.ts",
            "!src/**/*.test.ts"
        ],
        "dts": true,
        "format": [
            "cjs",
            "esm"
        ],
        "onSuccess": "npx tsc --emitDeclarationOnly --declaration"
    },
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup",
        "test": "jest",
        "test:watch": "jest --watch",
        "eslint": "eslint"
    },
    "jest": {
        "preset": "@deneb-viz/jest-presets/jest/node"
    },
    "dependencies": {
        "@deneb-viz/dataset": "*"
    },
    "devDependencies": {
        "@deneb-viz/core-dependencies": "*",
        "@deneb-viz/eslint-config": "*",
        "@deneb-viz/jest-presets": "*",
        "@deneb-viz/typescript-config": "*",
        "powerbi-visuals-utils-formattingmodel": "^6.0.2"
    }
}