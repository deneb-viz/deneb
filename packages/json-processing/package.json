{
    "name": "@deneb-viz/json-processing",
    "version": "0.0.1",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/deneb-viz/deneb.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./field-tracking": {
            "types": "./dist/lib/field-tracking/index.d.ts",
            "default": "./dist/lib/field-tracking/index.js"
        },
        "./spec-processing": {
            "types": "./dist/lib/spec-processing/index.d.ts",
            "default": "./dist/lib/spec-processing/index.js"
        },
        "./template-processing": {
            "types": "./dist/lib/template-processing/index.d.ts",
            "default": "./dist/lib/template-processing/index.js"
        }
    },
    "sideEffects": false,
    "scripts": {
        "build": "npm run build:worker && npm run build:package",
        "build:package": "tsup",
        "build:worker": "tsup --config tsup.worker.config.ts",
        "dev": "concurrently \"npm run dev:worker\" \"npm run dev:package\"",
        "dev:package": "tsup --watch --config tsup.config.ts",
        "dev:worker": "tsup --watch --config tsup.worker.config.ts",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit",
        "eslint": "eslint .",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "devDependencies": {
        "@deneb-viz/eslint-config": "*",
        "@deneb-viz/jest-presets": "*",
        "@deneb-viz/typescript-config": "*",
        "@types/lodash": "^4.14.202",
        "@types/node": "^22.15.33",
        "concurrently": "^9.1.2",
        "tsup": "^8.5.0",
        "typescript": "^5.6.2",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@deneb-viz/dataset": "*",
        "@deneb-viz/powerbi-compat": "*",
        "@deneb-viz/template-usermeta": "*",
        "@deneb-viz/utils": "*",
        "@deneb-viz/vega-runtime": "*",
        "ajv": "^8.12.0",
        "ajv-formats": "^2.1.1",
        "jsonc-parser": "^3.2.1",
        "lodash": "^4.17.21",
        "mergician": "^2.0.2",
        "vega": "6.2.0",
        "vega-expression": "^5.1.1",
        "vega-lite": "6.4.1",
        "vscode-json-languageservice": "^5.3.11"
    },
    "peerDependencies": {
        "powerbi-visuals-api": "5.11.0"
    }
}