{
    "name": "@deneb-viz/monaco-custom",
    "version": "1.7.0",
    "license": "MIT",
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/deneb-viz/sadr.git"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./index.css": "./dist/index.css",
        "./json.worker": {
            "types": "./dist/json.worker.d.ts",
            "default": "./dist/json.worker.js"
        }
    },
    "sideEffects": [
        "./dist/index.css"
    ],
    "files": [
        "dist"
    ],
    "tsup": {
        "entry": [
            "./src/index.ts",
            "./src/json.worker.ts"
        ],
        "dts": true,
        "format": [
            "cjs"
        ],
        "onSuccess": "npx tsc --emitDeclarationOnly --declaration",
        "treeshake": true
    },
    "scripts": {
        "dev": "tsup --watch",
        "build": "tsup --clean --minify"
    },
    "devDependencies": {
        "@deneb-viz/typescript-config": "*",
        "monaco-editor": "0.45.0"
    }
}