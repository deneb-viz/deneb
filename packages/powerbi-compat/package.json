{
    "name": "@deneb-viz/powerbi-compat",
    "version": "1.9.0",
    "author": "Deneb contributors",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/deneb-viz/deneb.git"
    },
    "packageManager": "npm@10.3.0",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./formatting": {
            "types": "./dist/lib/formatting/index.d.ts",
            "default": "./dist/lib/formatting/index.js"
        },
        "./interactivity": {
            "types": "./dist/lib/interactivity/index.d.ts",
            "default": "./dist/lib/interactivity/index.js"
        },
        "./properties": {
            "types": "./dist/lib/properties/index.d.ts",
            "default": "./dist/lib/properties/index.js"
        },
        "./signals": {
            "types": "./dist/lib/signals/index.d.ts",
            "default": "./dist/lib/signals/index.js"
        },
        "./theme": {
            "types": "./dist/lib/theme/index.d.ts",
            "default": "./dist/lib/theme/index.js"
        },
        "./visual-host": {
            "types": "./dist/lib/visual-host/index.d.ts",
            "default": "./dist/lib/visual-host/index.js"
        }
    },
    "sideEffects": false,
    "scripts": {
        "build": "npm run clean && tsc -p tsconfig.json",
        "dev": "tsc -p tsconfig.json --watch",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit",
        "eslint": "eslint ."
    },
    "devDependencies": {
        "@deneb-viz/eslint-config": "*",
        "@deneb-viz/typescript-config": "*",
        "@types/node": "^22.15.33",
        "powerbi-visuals-api": "5.11.0",
        "typescript": "^5.6.2",
        "vitest": "^3.2.4"
    },
    "peerDependencies": {
        "powerbi-visuals-api": "5.11.0"
    },
    "dependencies": {
        "@deneb-viz/configuration": "*",
        "@deneb-viz/utils": "*",
        "fast-equals": "^5.3.3",
        "powerbi-visuals-utils-formattingmodel": "^6.2.2",
        "powerbi-visuals-utils-formattingutils": "^6.1.2"
    }
}