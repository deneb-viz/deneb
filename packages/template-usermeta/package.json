{
    "name": "@deneb-viz/template-usermeta",
    "version": "1.9.0",
    "author": "Deneb contributors",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/deneb-viz/deneb.git"
    },
    "packageManager": "npm@10.3.0",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./schema.deneb-template-usermeta.json": "./dist/schema.deneb-template-usermeta.json"
    },
    "sideEffects": false,
    "scripts": {
        "build": "npm run build:schema && npm run build:package",
        "build:package": "tsc -p tsconfig.json",
        "build:schema": "node bin/generate-project-schema.js",
        "dev": "concurrently \"npm run dev:schema\" \"npm run dev:package\"",
        "dev:package": "tsc -p tsconfig.json --watch",
        "dev:schema": "tsx watch --include ./src/project/types.ts --clear-screen=false ./bin/generate-project-schema.js",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit -p tsconfig.json",
        "eslint": "eslint ."
    },
    "devDependencies": {
        "@deneb-viz/data-core": "*",
        "@deneb-viz/eslint-config": "*",
        "@deneb-viz/powerbi-compat": "*",
        "@deneb-viz/typescript-config": "*",
        "@deneb-viz/vega-runtime": "*",
        "@types/node": "^22.15.33",
        "ts-json-schema-generator": "^2.4.0",
        "tsx": "^4.20.3",
        "typescript": "^5.6.2",
        "vitest": "^3.2.4"
    }
}