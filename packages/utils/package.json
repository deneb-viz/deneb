{
    "name": "@deneb-viz/utils",
    "version": "0.1.0",
    "private": false,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/deneb-viz/deneb.git"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    },
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./dom": {
            "types": "./dist/lib/dom.d.ts",
            "default": "./dist/lib/dom.js"
        },
        "./inspection": {
            "types": "./dist/lib/inspection.d.ts",
            "default": "./dist/lib/inspection.js"
        },
        "./type-conversion": {
            "types": "./dist/lib/type-conversion.d.ts",
            "default": "./dist/lib/type-conversion.js"
        },
        "./worker": {
            "types": "./dist/lib/worker.d.ts",
            "default": "./dist/lib/worker.js"
        }
    },
    "sideEffects": false,
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit",
        "eslint": "eslint .",
        "test": "vitest run",
        "test:watch": "vitest"
    },
    "devDependencies": {
        "@deneb-viz/eslint-config": "*",
        "@deneb-viz/typescript-config": "*",
        "@types/node": "^22.15.33",
        "tsup": "^8.5.0",
        "typescript": "^5.6.2",
        "vitest": "^3.2.4"
    }
}