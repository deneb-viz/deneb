{
    "name": "@deneb-viz/vega-runtime",
    "version": "1.9.0",
    "author": "Deneb contributors",
    "private": true,
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/deneb-viz/deneb.git"
    },
    "packageManager": "npm@10.3.0",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./embed": {
            "types": "./dist/lib/embed/index.d.ts",
            "default": "./dist/lib/embed/index.js"
        },
        "./extensibility": {
            "types": "./dist/lib/extensibility/index.d.ts",
            "default": "./dist/lib/extensibility/index.js"
        },
        "./pattern-fill": {
            "types": "./dist/lib/pattern-fill/index.d.ts",
            "default": "./dist/lib/pattern-fill/index.js"
        },
        "./provenance": {
            "types": "./dist/lib/provenance/index.d.ts",
            "default": "./dist/lib/provenance/index.js"
        }
    },
    "sideEffects": false,
    "scripts": {
        "build": "tsup",
        "dev": "tsup --watch",
        "clean": "rimraf dist",
        "typecheck": "tsc --noEmit",
        "eslint": "eslint ."
    },
    "devDependencies": {
        "@deneb-viz/eslint-config": "*",
        "@deneb-viz/typescript-config": "*",
        "@types/node": "^22.15.33",
        "tsup": "^8.5.0",
        "typescript": "^5.6.2",
        "vitest": "^3.2.4"
    },
    "dependencies": {
        "@deneb-viz/configuration": "*",
        "@deneb-viz/utils": "*",
        "d3-interpolate": "^3.0.1",
        "d3-selection": "^3.0.0",
        "mergician": "^2.0.2",
        "powerbi-visuals-utils-formattingutils": "^6.1.2",
        "vega": "6.2.0",
        "vega-lite": "6.4.1"
    },
    "peerDependencies": {
        "@deneb-viz/powerbi-compat": "*",
        "powerbi-visuals-api": "5.11.0"
    }
}
